local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Beyond Hub", "GrapeTheme")
local Tab = Window:NewTab("script")
local Section = Tab:NewSection("script")
Section:NewButton("+1 Speed Per RNG", "ButtonInfo", function()
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Beyond Hub", "GrapeTheme")
local Tab = Window:NewTab("Farm")
local Section = Tab:NewSection("farm")

getgenv().AutoFarm1 = false
getgenv().AutoFarm2 = false
getgenv().AutoFarm3 = false
getgenv().AutoFarm4 = false
getgenv().AutoFarm5 = false
getgenv().AutoFarm6 = false

-- ðŸŒ€ Toggle 1
Section:NewToggle("1 (5)", "Teleport", function(state)
    getgenv().AutoFarm1 = state
    if state then
        print("Farm 1 On")
        task.spawn(function()
            while getgenv().AutoFarm1 do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                    CFrame.new(-37.9999542, 0.619660378, 320.999939, -1, 0, 0, 0, 1, 0, 0, 0, -1)
                task.wait(0.5)
            end
        end)
    else
        print("Farm 1 Off")
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                    CFrame.new(0, 15.6965437, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                task.wait(0.5)
    end
end)

-- ðŸŒ€ Toggle 2
